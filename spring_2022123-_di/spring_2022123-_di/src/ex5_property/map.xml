	<bean id="map" class="ex5_property.Ex2_Map" abstract="true">
		<property name="map">
			<map>
				<entry key="email" value="kkkkkk11@naver.com" />
				<entry key="name" value="김길동" />
				<entry key="name" value="우히히" />
			</map>
		</property>
	</bean>

</beans>
